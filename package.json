    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.0.2",
    "jest": "^26.6.3",
    "mongodb-memory-server": "^6.9.6",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "apollo-server": "^2.25.1",
    "apollo-server-express": "^2.25.1",
