{
  "name": "jumbo-stores",
  "version": "1.1.0",
  "description": "This is Jumbo's Assignement Project",
  "main": "dist/bin/server.js",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
